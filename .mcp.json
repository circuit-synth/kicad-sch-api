{
  "mcpServers": {
    "context-portal": {
      "command": "uv",
      "args": [
        "run",
        "--project",
        "tools/context-portal",
        "python",
        "-m",
        "context_portal_mcp.main",
        "--mode",
        "stdio",
        "--workspace_id",
        "${PWD}",
        "--log-file",
        "../../logs/conport.log",
        "--log-level",
        "INFO"
      ],
      "env": {}
    }
  }
}